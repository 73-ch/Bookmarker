{
  "manifest_version": 2,
  "name": "Tab Manager",
  "version": "0.0.0.0",

  "browser_action": {
    "default_popup":"popup.html",
    "default_title": ""
  },

  "background": {
     "scripts": ["background.js"],
     "persistent": false
  },

  "permissions" : [
    "tabs",
     "bookmarks",
     "activeTab"
 ]
  }
